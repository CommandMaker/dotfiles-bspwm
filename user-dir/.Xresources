! URxvt config
!URxvt*background: [70]#1a1b26
URxvt*scrollTtyKeypress: true
URxvt.font: xft:JetBrains Mono:size=13, \
            xft:Font Awesome 6 Free:pixelsize=14:size=20, \
            xft:Dejavu Sans Mono:pixelsize=20:size=20:minspace=False, \
            xft:Inconsolata Nerd Font Mono:pixelsize=20:size=20:minspace=False, \
            xft:FreeMono:pixelsize=15:size=20:minspace=False, \
            xft:Noto Emoji:minspace=False
URxvt*scrollBar: false
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.perl-ext-common: default,vtwheel,clipboard,matcher,resize-font,-confirm-paste
URxvt.clipboard.autocopy: false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Control-Insert: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: eval:paste_clipboard

URxvt.url-launcher: /usr/bin/brave
URxvt.matcher.button: 1
! URxvt config ends

Xcursor.theme: Bibata-Modern-Ice


